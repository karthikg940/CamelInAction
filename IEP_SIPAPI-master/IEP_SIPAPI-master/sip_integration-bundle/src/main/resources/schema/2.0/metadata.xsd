<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" 
    xmlns:odm="http://www.cdisc.org/ns/odm/v1.3" 
    xmlns:sip="http://www.sharedinvestigator.com/sip/schema" 
    targetNamespace="http://www.sharedinvestigator.com/sip/schema" 
    elementFormDefault="qualified" attributeFormDefault="unqualified">
    
    <xs:import namespace="http://www.cdisc.org/ns/odm/v1.3" schemaLocation="odm1_3_2/ODM1-3-2.xsd"/>

    <xs:complexType name="metadata">
        <xs:annotation>
            <xs:documentation> Pagination of Entities </xs:documentation>
        </xs:annotation>		
        <xs:sequence>
            <xs:element type="odm:integer"  name="Offset" minOccurs="0"/>
            <xs:element type="odm:integer" name="Limit" minOccurs="0" />
            <xs:element type="odm:integer" name="Totalcount" minOccurs="0"  />             
        </xs:sequence>
    </xs:complexType>
</xs:schema>
